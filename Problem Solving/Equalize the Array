int equalizeArray(vector<int> arr) {
int mx=INT_MIN;
for(int i=0;i<arr.size();i++){
    vector<int>arr2=arr;
    int count=0;
for(int j=0;j<arr2.size();j++){
      arr2[j]-=arr[i];
  }
  for(int j=0;j<arr2.size();j++){
      if(arr2[j]==0)
      count++;
  }
  mx=max(count,mx);
}
return arr.size()-mx;
}
