void plusMinus(vector<int> arr) {
    int n=arr.size();
int pos=0,neg=0;
for(int i=0;i<n;i++){
    if(arr[i]>0){
    pos++;
    }
    else if(arr[i]<0)
    neg++;
}
double pratio=(double)pos/(double)n;
double nratio=(double)neg/(double)n;
double zratio=1-pratio-nratio;
cout<<fixed<<setprecision(6)<<pratio<<endl;
cout<<fixed<<setprecision(6)<<nratio<<endl;
