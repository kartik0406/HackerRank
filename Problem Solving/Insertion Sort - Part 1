void insertionSort1(int n, vector<int> arr) {
  int ele=arr[n-1],b=-1;
  for(int i=n-2;i>=0;i--){ 
      if(ele<arr[i])
        arr[i+1]=arr[i];
      else if(arr[i]<ele){
         b=i+1;     
         break;
      }
for(int j=0;j<n;j++)
      cout<<arr[j]<<" ";
      cout<<endl;
  }
  if(b==-1)
   b=0;
  arr[b]=ele;
 for(int j=0;j<n;j++)
       cout<<arr[j]<<" ";
       cout<<endl;
}
