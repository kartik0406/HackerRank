void insertionSort2(int n, vector<int> arr){
for(int i=1;i<n;i++) {
		int ele=arr[i];
		int b=i;
		for(int j=0;j<i;j++){
			if(ele<arr[j])
				{b=j;
				 break;
		    }
		}
	   for(int k=i;k>b;k--) {
	   	arr[k]=arr[k-1];
	   }
	   arr[b]=ele;
	  for(int c=0;c<n;c++) {
	  	cout<<arr[c]<<" ";
	  }
	  cout<<endl;;
	}
}
