int hourglassSum(vector<vector<int>> arr) {
int m=0,k=0,max=-200;

for(int i=0;i<4;i++){
    int temp=0;
     k=0;
    for(int j=0;j<4;j++){
    temp=0;
       temp+=arr[m][k]+arr[m][k+1]+arr[m][k+2]+arr[m+1][k+1]+arr[m+2][k]+arr[m+2][k+1]+arr[m+2][k+2];
        k++;
   
        if(max<temp)
            max=temp;
    }
   m++; 
}
return max;;
}
