bool compare_lists(SinglyLinkedListNode* head1, SinglyLinkedListNode* head2) {
int flag=1;
while(head1!=NULL && head2!=NULL){
 
    if(head1->data!=head2->data)
     {
         flag=0;
         break;
     }
    head1=head1->next;
    head2=head2->next;
}
if(head1!=NULL || head2!=NULL){
    flag=0;
}
return flag;
}
